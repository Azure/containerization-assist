{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "entry": [
        "src/index.ts",
        "src/mcp/mcp-server.ts",
        "docs/examples/**/*.{ts,js}",
        "test/**/*.test.ts",
        "scripts/**/*.ts"
    ],
    "project": [
        "src/**/*.ts"
    ],
    "ignore": [
        "**/*.test.ts",
        "**/*.spec.ts",
        "scripts/**/*.js",
        "docs/examples/**/*.ts",
        "src/lib/param-defaults.ts",
        "src/lib/result-utils.ts"
    ],
    "ignoreExportsUsedInFile": {
        "interface": true,
        "type": true
    },
    "ignoreDependencies": [
        "husky",
        "@kubernetes-models/apimachinery",
        "dockerfilelint",
        "kubernetes-models"
    ],
    "ignoreBinaries": [
        "opa"
    ],
    "includeEntryExports": false
}