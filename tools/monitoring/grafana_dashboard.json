{
  "dashboard": {
    "id": null,
    "title": "Container Kit Performance Dashboard",
    "tags": ["container-kit", "performance", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Tool Execution Latency",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(tool_execution_duration_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(tool_execution_duration_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.0002},
                {"color": "red", "value": 0.0003}
              ]
            },
            "unit": "s"
          }
        }
      },
      {
        "id": 2,
        "title": "Tool Execution Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(tool_execution_total[1m])",
            "legendFormat": "{{ tool_name }}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "xAxis": {"show": true},
        "yAxes": [
          {
            "label": "executions/sec",
            "show": true,
            "min": 0
          }
        ],
        "legend": {
          "avg": false,
          "current": false,
          "max": false,
          "min": false,
          "show": true,
          "total": false,
          "values": false
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(tool_errors_total[1m])",
            "legendFormat": "{{ tool_name }} errors",
            "refId": "A"
          },
          {
            "expr": "rate(pipeline_errors_total[1m])",
            "legendFormat": "{{ pipeline_type }} pipeline errors",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "yAxes": [
          {
            "label": "errors/sec",
            "show": true,
            "min": 0
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [0.1],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "1m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "2m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Error Rate",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 4,
        "title": "Pipeline Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(pipeline_execution_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Pipeline Duration",
            "refId": "A"
          },
          {
            "expr": "rate(pipeline_stage_execution_total[1m])",
            "legendFormat": "Stage Execution Rate",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "yAxes": [
          {
            "label": "seconds",
            "show": true,
            "min": 0
          }
        ]
      },
      {
        "id": 5,
        "title": "HTTP Request Latency",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(http_request_duration_seconds_bucket[5m])",
            "legendFormat": "{{ le }}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "heatmap": {
          "xAxis": {
            "show": true
          },
          "yAxis": {
            "show": true,
            "label": "Duration (s)",
            "min": "0",
            "max": "1"
          },
          "colorScale": "exponential",
          "colorScheme": "interpolateSpectral"
        }
      },
      {
        "id": 6,
        "title": "System Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "system_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)",
            "refId": "A"
          },
          {
            "expr": "system_cpu_usage_percent",
            "legendFormat": "CPU Usage (%)",
            "refId": "B"
          },
          {
            "expr": "system_goroutines_count",
            "legendFormat": "Goroutines",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "yAxes": [
          {
            "show": true,
            "min": 0
          }
        ]
      },
      {
        "id": 7,
        "title": "Session Metrics",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(session_total[1h])",
            "legendFormat": "Sessions Created/hour",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(session_duration_seconds_bucket[1h]))",
            "legendFormat": "P95 Session Duration",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 24},
        "options": {
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 8,
        "title": "Top Tools by Execution Count",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, rate(tool_execution_total[1h]))",
            "legendFormat": "{{ tool_name }}",
            "refId": "A",
            "format": "table"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "options": {
          "showHeader": true
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Slowest Tools (P95 Latency)",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.95, rate(tool_execution_duration_seconds_bucket[1h])))",
            "legendFormat": "{{ tool_name }}",
            "refId": "A",
            "format": "table"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "options": {
          "showHeader": true
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.0002},
                {"color": "red", "value": 0.0003}
              ]
            }
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "datasource": "${DS_PROMETHEUS}",
          "definition": "label_values(tool_execution_total, tool_name)",
          "hide": 0,
          "includeAll": true,
          "label": "Tool",
          "multi": true,
          "name": "tool",
          "options": [],
          "query": "label_values(tool_execution_total, tool_name)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 1,
          "tagValuesQuery": "",
          "tags": [],
          "tagsQuery": "",
          "type": "query",
          "useTags": false
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "links": []
  }
}
